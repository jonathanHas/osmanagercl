APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

# POS Database Connection (uniCenta)
# Uncomment and configure when ready to connect to POS system
# POS_DB_HOST=127.0.0.1
# POS_DB_PORT=3306
# POS_DB_DATABASE=unicenta
# POS_DB_USERNAME=readonly_user
# POS_DB_PASSWORD=

# OSAccounts Integration
# Path to OSAccounts invoice storage files (for attachment import)
OSACCOUNTS_FILE_PATH=/var/www/html/OSManager/invoice_storage

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Udea Supplier Integration
# Uncomment and configure when ready to test Udea scraping
# UDEA_BASE_URI=https://www.udea.nl
# UDEA_USERNAME=your_username
# UDEA_PASSWORD=your_password
# UDEA_TIMEOUT=30
# UDEA_RATE_LIMIT_DELAY=2
# UDEA_CACHE_TTL=3600
